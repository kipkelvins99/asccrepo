<?xml version="1.0" encoding="utf-8" ?>
<odoo>
    <record id="revenue_form_inherit" model="ir.ui.view">
        <field name="name">Revenue Start Date</field>
        <field name="model">account.asset</field>
        <field name="inherit_id" ref="account_asset.view_account_asset_revenue_form"/>
        <field name="arch" type="xml">
            <xpath expr="//field[@name='first_depreciation_date']" position="before">
                <field name="recognize_on_first"/>
            </xpath>
        </field>
    </record>

</odoo>